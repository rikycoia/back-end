package it.epicode.multimedia.player;

//creazione classe per i file video
//e implementa l'interfaccia

public class Video extends Multimedia implements RiproduciElemento {

	private double luminosità;
	private double durata;
	private double volume;

	public Video(String title, double luminosità, double durata, double volume) {

		super(title);
		this.luminosità = luminosità;
		this.durata = durata;
		this.volume = volume;

	}
	
	public void alzaVolume(double volume) {
		
		this.volume = volume++;
				
	}

	@Override
	public void play() {
		// TODO Auto-generated method stub
		
	}

}
