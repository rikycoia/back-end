
public class Switch {

	private	int numero; 
	
	public static void between(int numero) {
		switch(numero) {
		  case 1:
		    System.out.println("è uno");
		    break;
		  case 2:
			  System.out.println("è due");
		  case 3:
			  System.out.println("è tre");
		    break;
		  default:
			  System.out.println("sei un mona");
		}
		
	}
			
}
